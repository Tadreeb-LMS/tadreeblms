{
    "name": "your-project/tadreeblms",
    "type": "project",
    "description": "Tadreeb LMS",
    "keywords": ["laravel", "LMS"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^9.52",
        "spatie/laravel-html": "^2.30",
        "barryvdh/laravel-dompdf": "^2.0",
        "yajra/laravel-datatables-oracle": "^9.0",
        "yajra/laravel-datatables-buttons": "^4.13",
        "yajra/laravel-datatables-html": "^4.41",
        "appstract/laravel-blade-directives": "^1.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "fakerphp/faker": "^1.21",
        "barryvdh/laravel-debugbar": "^3.7"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.4.15"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
